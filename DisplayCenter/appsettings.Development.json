{
  "Logging": {
    "LogLevel": {
      "Default": "Trace",
      "Microsoft.EntityFrameworkCore": "Warning"
    }
  },
  "MutexName": "DCUI",
  "Window": {
    "Caption": "纪念币实时显示系统",
    "CaptionFontSize": 25,
    "TopMost": false,
    "ExtensionAreaShowOnStartup": false
  },
  "SolutionCluster": {
    "SolutionCardView": {
      "Height": 450,
      "Width": 550,
      "FontSize": 17,
      "ShowImageViewStatusBar": false,
      "ShowImageInfoColor": true,
      "ShowRoiDescription": false,
      "FlowVelocityUnit": "枚/分钟"
    },
    "ClassifyView": {
      "CardHeight": 150,
      "CardWidth": 150
    },
    "Solutions": [
      {
        "Id": 100,
        "Key": "SouthSecurityLine",
        "Display": "南线安全线",
        "Order": 100,
        "ClassifyGroups": [ 100, 101, 1000 ]
      },
      {
        "Id": 101,
        "Key": "NorthSecurityLine",
        "Display": "北线安全线",
        "Order": 101,
        "ClassifyGroups": [ 100, 101, 1000 ]
      }
    ],
    "DefaultSolution": {
      "Id": 100000000,
      "Key": "未命名",
      "Display": "未命名工位",
      "Order": 1000000,
      "ClassifyGroups": [ 9999999 ]
    },
    "DefaultClassifyGroup": {
      "Id": 99999990,
      "Display": "默认组",
      "Color": "primary brown",
      "MaxCacheCount": 20,
      "DisableDisplayInFront": false,
      "AlterKey": "------------"
    },
    "ClassifyGroups": [
      {
        "Id": 1000,
        "Display": "初始化",
        "Color": "accent purple",
        "MaxCacheCount": 0,
        "DisableDisplayInFront": false,
        "AlterKey": "Init"
      },
      {
        "Id": 100,
        "Display": "Ok",
        "Color": "primary green",
        "MaxCacheCount": 20,
        "DisableDisplayInFront": false,
        "AlterKey": "-ok",
        "AddToSummaryGroup": "OK"
      },
      {
        "Id": 101,
        "Display": "Ng",
        "Color": "primary red",
        "MaxCacheCount": 20,
        "DisableDisplayInFront": false,
        "AlterKey": "-error",
        "AddToSummaryGroup": "NG"
      }
    ]
  },
  "ImageService": {
    "PollTimeout": 20,
    "Endpoints": [
      {
        "Id": 1,
        "Name": "file",
        "Address": "ipc://ui"
      },
      {
        "Id": 2,
        "Name": "private net",
        "Address": "tcp://*:12306"
      }
    ],
    "BoundedChannelCapacity": 200
  },
  "Scanner": {
    "Core": {
      "FileReadSpinWaitTimeout": 500,
      "Groups": [
        {
          "Name": "北线安全线检测/Init",
          "Monitor": {
            "FileNameKey": "NorthSecurityLine-init"
          },
          "Operations": [
            {
              //"EnableNetTransfer": true,
              //"NetAddress": "tcp://127.0.0.1:12308"
              "InProcessTransferKey": "imagedto"
            }
          ]
        },
        {
          "Name": "北线安全线检测/Ng",
          "Monitor": {
            "FileNameKey": "NorthSecurityLine-error"
          },
          "Operations": [
            {
              "EnableNetTransfer": true,
              "NetAddress": "tcp://127.0.0.1:12308"
              //"InProcessTransferKey": "imagedto"
            },
            {
              "EnableBackup": true
            }
          ]
        },
        {
          "Name": "北线安全线检测/Ok",
          "Monitor": {
            "FileNameKey": "NorthSecurityLine-ok"
          },
          "Operations": [
            {
              //"EnableNetTransfer": true
              "InProcessTransferKey": "imagedto"
            },
            {
              "EnableBackup": true
            }
          ]
        },
        {
          "Name": "南线安全线检测/Init",
          "Monitor": {
            "FileNameKey": "SouthSecurityLine-init"
          },
          "Operations": [
            {
              //"EnableNetTransfer": true,
              //"NetAddress": "tcp://127.0.0.1:12308"
              "InProcessTransferKey": "imagedto"
            }
          ]
        },
        {
          "Name": "南线安全线检测/Ng",
          "Monitor": {
            "FileNameKey": "SouthSecurityLine-error"
          },
          "Operations": [
            {
              //"EnableNetTransfer": true
              "InProcessTransferKey": "imagedto"
            },
            {
              "EnableBackup": true
            }
          ]
        },
        {
          "Name": "南线安全线检测/Ok",
          "Monitor": {
            "FileNameKey": "SouthSecurityLine-ok"
          },
          "Operations": [
            {
              //"EnableNetTransfer": true
              "InProcessTransferKey": "imagedto"
            },
            {
              "EnableBackup": true
            }
          ]
        },
        {
          "Name": "Backup2NewDir",
          "Monitor": {
            "FileNameKey": "Backup2NewDir"
          },
          "Operations": [
            {
              //"EnableNetTransfer": true
              "InProcessTransferKey": "imagedto"
            },
            {
              "EnableBackup": true,
              "BackupPath": ".\\Image_Backup_New"
            }
          ]
        },
        {
          "Name": "Move2Dir",
          "Monitor": {
            "FileNameKey": "Move2Dir"
          },
          "Operations": [
            {
              //"EnableNetTransfer": true
              "InProcessTransferKey": "imagedto"
            },
            {
              "EnableMove": true,
              "MovePath": ".\\Image_Move"
            }
          ]
        },
        {
          "Name": "Ftp",
          "Monitor": {
            "FileNameKey": "FtpDelete"
          },
          "Operations": [
            {
              "EnableFtpTransfer": true,
              "DisableFileNameAddedTimeStamp": true
            },
            {
              //"EnableNetTransfer": true
              "InProcessTransferKey": "imagedto"
            },
            {
              "EnableBackup": true,
              "DisableFileNameAddedTimeStamp": false
            }
          ]
        }
      ],
      "Default": {
        "Monitor": {
          "Path": ".\\Images",
          "FileNameKey": "_",
          "FileExtensions": [ "png", "bmp", "jpg" ]
        },
        "Operation": {
          "NetAddress": "ipc://ui",
          "MovePath": ".\\Move",
          "BackupPath": ".\\Image_Backup",
          "Ftp": {
            "FtpRoot": "ftp://my-lenovo.inner.private.com:21",
            "UserName": "User",
            "Password": "user",
            "Timeout": 500
          }
        }
      },
      "FileMan": {
        "ScanPeriod": 200
      }
    }
  }
}